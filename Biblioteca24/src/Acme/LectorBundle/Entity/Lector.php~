<?php

namespace LectorBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Lector
 *
 * @ORM\Table(name="lector")
 * @ORM\Entity
 */
class Lector
{
    /**
     * @var string
     *
     * @ORM\Column(name="rut", type="string", length=10, nullable=false)
     * @ORM\Id
     */
    private $rut;

    /**
     * @var string
     *
     * @ORM\Column(name="nombre", type="string", length=100, nullable=false)
     */
    private $nombre;

    /**
     * @var string
     *
     * @ORM\Column(name="sexo", type="string", length=10, nullable=false)
     */
    private $sexo;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="fnac", type="date", nullable=false)
     */
    private $fnac;

    /**
     * @var string
     *
     * @ORM\Column(name="direccion", type="string", length=45, nullable=true)
     */
    private $direccion;

    /**
     * @var integer
     *
     * @ORM\Column(name="telefono", type="integer", nullable=false)
     */
    private $telefono;

    /**
     * @var string
     *
     * @ORM\Column(name="email", type="string", length=45, nullable=true)
     */
    private $email;


}

